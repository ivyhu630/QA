CREATE TABLE `QuestionList` (
  `id` INTEGER NULL AUTO_INCREMENT DEFAULT NOT NULL,
  `product_id` INTEGER NULL DEFAULT NOT NULL,
  PRIMARY KEY (`id`)
);
CREATE TABLE QuestionList (
  id INTEGER NULL AUTO_INCREMENT DEFAULT NOT NULL,
  product_id INTEGER NULL DEFAULT NOT NULL,
  PRIMARY KEY (id)
);
CREATE TABLE QuestionList (
  id SERIAL,
  product_id INTEGER DEFAULT NOT NULL,
  PRIMARY KEY (id)
);
CREATE TABLE QuestionList (
  id SERIAL,
  product_id INTEGER NOT NULL,
  PRIMARY KEY (id)
);
DROP TABLE IF EXISTS QuestionList;
CREATE TABLE AnswerList (
  id SERIAL,
  question_id INTEGER NOT NULL,
  PRIMARY KEY (id)
);
CREATE TABLE Answers (
  id SERIAL,
  question_id BIGINT,
  body TEXT NOT NULL,
  date_written BIGINT,
  answerer_name VARCHAR(20),
  email VARCHAR(20),
  reported INTEGER CHECK (reported >= 0),
  helpful INTEGER CHECK (helpful >= 0),
  PRIMARY KEY (id)
);
