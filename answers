CREATE DATABASE qa;
\c qa
CREATE TABLE Answers (
  id SERIAL,
  question_id BIGINT,
  body TEXT NOT NULL,
  date_written BIGINT,
  answerer_name VARCHAR(20),
  email VARCHAR(20),
  reported INTEGER CHECK (reported >= 0),
  helpful INTEGER CHECK (helpful >= 0),
  PRIMARY KEY (id)
);
